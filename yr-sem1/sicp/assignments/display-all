#lang racket
;;QTN 5
;; Custom display-all function

(define (insert-between v xs)
  (cond ((null? xs) xs)
        ((null? (cdr xs)) xs)
        (else (cons (car xs)
                    (cons v (insert-between v (cdr xs)))))))

(define (display-all . vs)
  (for-each display (insert-between  " " vs))
  (newline))

(display-all "foo")
(display-all "foo" "bar")