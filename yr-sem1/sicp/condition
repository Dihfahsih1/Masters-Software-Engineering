;qtn 1
(define a 3)  
a ;(=a 2)

;qtn2
(define b(+ a 1))
b ; (= b 3)

;qtn 3
(+ a b (* a b))

;qtn 4
(= a b)

;qtn 5
(if(> a b) a b);answer is 3

;qtn 6
(if (and (> a b) (< b (* a b)))
b a); answer is 2 because the AND predicates stops evaluating when the the 1st cond is false

;qtn 7
(+ 2 (if (> a b) a b))
;It returns 5

;qtn 8
(* (cond ((> a b) a);this is false so it is skipped
         ((< a b) b); this is true so it is evaluated and returns 4
         (else -1)); this is skipped
   (+ a 1)); (* 3 (+ 2 1)) = 16

;If a person is above 18, print a message that allows them to vote, otherwise print an error message
(define person 18)

( if(= person 18) "yes vote" "error")

;A certain university uses the following criteria for grading students:
;if mark is greater than 80, give an A
;If mark is between 65 and 80, give a B
;If mark is between 50 and 65, give a C
;Otherwise, the student has failed.
;Write a Scheme expression to grade students.

(define marks 0)
(define (grade marks)
(if(> marks 80) "A"
   (if (> marks 65)(< marks 80) "B")
))
(grade 60)
(define (sum-integers a b)
  (if(> a b)
     0
    (+ a(sum-integers (+ a 1) b))))
(sum-integers 4 3)