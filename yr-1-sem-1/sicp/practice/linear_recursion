#reader(lib "htdp-beginner-reader.ss" "lang")((modname linear_recursion) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
;linear recursive process for factorial 6!
(define (factorial n)
  (if (= n 1)
      1
      (* n(factorial (- n 1))))
  )

(factorial 6)

;linear iterative process for factorial of 6

;exercise 2.1

(define (make-rat n d)
  (let ((g((if(< d 0)- +) (abs(gcd n d)))))
    cons (/ n g) (/ d g)))


  
